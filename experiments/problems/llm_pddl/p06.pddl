(define (problem blocksworld-4ops-problem)
  (:domain blocksworld-4ops)
  (:objects b1 b2 b3 b4 b5)
  (:init (clear b2)
         (clear b3)
         (clear b4)
         (clear b5)
         (on-table b1)
         (arm-empty)
         (on b2 b1)
         (on b3 b4)
         (on b4 b2)
         (on b5 b3))
  (:goal (and (on b1 b2)
              (on b3 b5)
              (on b4 b1))))